(declare-var x_0 Int)
(declare-var y_0 Int)
(declare-var x_1 Int)
(declare-var y_1 Int)
(declare-rel p0 ())
(declare-rel p1 (Int Int))
(declare-rel p2 (Int Int))
(declare-rel p3 (Int Int))
(declare-rel p4 ())
(declare-rel error ())
(rule p0)
(rule (=> (and p0 (= x_0 1) (= y_0 0))
  (p1 x_0 y_0)
))
(rule (=> (p1 x_0 y_0)
  (p2 x_0 y_0)
))
(rule (=> (p1 x_0 y_0)
  (p3 x_0 y_0)
))
(rule (=> (and (p2 x_0 y_0) (= x_1 (+ x_0 y_0)) (= y_1 (+ 1 y_0)))
  (p1 x_1 y_1)
))
(rule (=> (and (p3 x_0 y_0) (>= x_0 y_0))
  p4
))
(rule (=> p4
  p4
))
(rule (=> (and (p3 x_0 y_0) (< x_0 y_0))
  error
))
(query error)
